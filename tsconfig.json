{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "declarations.d.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.tsx",
    "constants/icons.js",
    "app/(Dashboard)/Notifications.jsx"
, "app/auth/ForgotPassword.tsx"  ]
}
